{
  "id": "https://w3id.org/dats/schema/access_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Dom's second test template schema",
  "description": "A simple test to create a template and upload it throught CEDAR API",
  "type": "object",
  "properties": {
    "@context": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object"
        }
      ]
    },
    "@id": { "type": "string", "format": "uri" },
    "@type": { "type": "string", "enum": [ "Access" ]},
    "Title": {
      "type": "array",
      "description": "Title field schema generated by the CEDAR Template Editor 1.8.2-SNAPSHOT"
    }
  },
  "additionalProperties": false,
  "required": [ "Title" ]
}